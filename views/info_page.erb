<h1 class = "font_size cent font_type"> Phone Book App</h1> 



<div id = "box" class = "font_size cent">
<form method = "post" action = "results">
	<input type = "text" name = "info[]" placeholder="First Name" required>
	<input type = "text" name = "info[]" placeholder="Last Name" required><br>
	<input type = "text" name = "info[]" placeholder="Street Address" required>
	<input type = "text" name = "info[]" placeholder="City" required><br>
	<input type = "text" name = "info[]" placeholder="State" required>
	<input type = "number" name = "info[]" placeholder="Zip Code" required><br>
	<input type = "number" name = "info[]" placeholder="Phone Number" required>
	<input type = "submit" value = "enter">
</form>
</div>


